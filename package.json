  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmblack/oscal-processor.git"
  },
  "keywords": [
    "OSCAL",
    "nist",
    "sp",
    "800",
    "53"
  ],
  "author": "Blackmore, Dustyn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmblack/oscal-processor/issues"
  },
  "homepage": "https://github.com/dmblack/oscal-processor#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.4.2",
    "semistandard": "^16.0.1"
  },
  "semistandard": {
    "ignore": [
      "**/setupTests.js"
    ]
  }
}
